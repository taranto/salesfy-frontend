	"scripts": {
		"postinstall": "node ./config/android-release-gradle-fix.js && rm -rf node_modules/react-native-sensitive-info/index.d.ts && cp -rf override/data-manager.js node_modules/material-table/dist/utils/",
		"start": "react-native start",
		"start-no-cache": "react-native start --reset-cache",
		"ios": "react-native run-ios",
		"ios-config-after-install": "cd node_modules/react-native/ && sudo scripts/ios-install-third-party.sh && cd third-party/glog-0.3.4 && sudo ../../scripts/ios-configure-glog.sh",
		"android": "react-native run-android",
		"android-linux": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
		"tsc": "tsc",
		"watch": "tsc --watch",
		"clear-artifacts": "rm -rf artifacts",
		"build": "npm run clear-artifacts && npm run tsc --",
		"tslintit": "tslint -p tsconfig.json",
		"lint": "tslint -p tsconfig.json -t verbose",
		"lint-fix": "tslint -p tsconfig.json --fix -t verbose",
		"precommit": "yarn test",
		"test": "jest --config jest.config.js --coverage",
		"dev-test": "jest --config jest.config.js --watchAll",
		"to-workspace-app": "yes | cp -rf config/tsconfig.app.json tsconfig.json && yes | cp -rf config/.app.babelrc .babelrc",
		"android-apk": "cd android && ./gradlew app:assembleRelease",
		"android-simple-apk": "cd android && ./gradlew assembleRelease",
		"run-android-apk": "sudo react-native run-android --variant=release",
		"to-workspace-web": "yes | cp -rf config/tsconfig.web.json tsconfig.json && yes | cp -rf config/.web.babelrc .babelrc",
		"prepare-web": "cp config/paths.js node_modules/react-scripts-ts/config/",
		"web": "yarn tsc --p tsconfig.json && REACT_APP_ENV='web' react-scripts-ts start --config=tsconfig.json --env=web",
		"web-dev": "NODE_ENV=development REACT_APP_ENV='web' webpack-dev-server --config=configs/webpack/dev.js --env=web",
		"clean-dist": "rm -f -r -d dist",
		"web-build": "set \"GENERATE_SOURCEMAP=false\" && REACT_APP_ENV='web' && yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
		"web-hatchers-deploy": "aws s3 sync dist/ s3://web.hatchers.com.br",
		"web-salesfy-deploy": "aws s3 sync dist/ s3://web.salesfy.com.br",
		"web-s3-deploy": "aws s3 sync dist/ s3://hatchers-web",
		"clear-project": "rm -rf node_modules js log coverage .nyc_output models extra-content salesfy-shared",
		"full-reinstall": "yarn clear-project && yarn shared-refresh && npm install",
		"shared-refresh": "yarn shared-bring-uninstalled-to-root && yarn shared-uninstall-as-module && yarn shared-install && yarn shared-compile && yarn shared-install-as-module",
		"shared-bring-uninstalled-to-root": "rm -rf ./salesfy-shared && rsync -av --progress ../salesfy-shared ./ --exclude node_modules --exclude js --exclude .git --exclude .npmrc --exclude .npmignore --exclude .gitignore",
		"shared-install": "cd ./salesfy-shared && npm install && cd ..",
		"shared-compile": "cd ./salesfy-shared && yarn compile-shared && cd ..",
		"shared-install-as-module": "npm install salesfy-shared@file:./salesfy-shared --save",
		"shared-uninstall-as-module": "npm uninstall salesfy-shared --save"
	},
	"rnpm": {
		"assets": [
			"assets/text",
			"assets/fonts"
		]
	},
	"engines": {
		"node": "9.11.2"
	}
